import("//build_overrides/llvm.gni")

static_library("Option") {
  output_name = "LLVMOption"
  deps = [ "//${llvm_root}llvm/lib/Support" ]
  sources = [
    "Arg.cpp",
    "ArgList.cpp",
    "OptTable.cpp",
    "Option.cpp",
  ]
  configs += llvm_code_override_configs_append
  configs -= llvm_code_override_configs_remove
}
